<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="NumView">
        <attr name="numViewTextSize" format="dimension"/>
        <attr name="numViewTextColor" format="color"/>
        <attr name="numViewText" format="string"/>
    </declare-styleable>
    <declare-styleable name="RoundProgressBar">
        <attr name="textColor" format="color"/>
        <attr name="textSize" format="dimension"/>
        <attr name="pb_bg_color" format="color"/>
        <attr name="pb_width" format="dimension"/>
        <attr name="pb_progress_color" format="color"/>
        <attr name="pb_max_progress" format="integer"/>
    </declare-styleable>
    <declare-styleable name="GifView">
        <attr name="gif" format="reference"/>
        <attr name="paused" format="boolean"/>
    </declare-styleable>
</resources>